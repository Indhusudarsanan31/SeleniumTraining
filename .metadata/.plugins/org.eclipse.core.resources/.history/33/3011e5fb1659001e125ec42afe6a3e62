package org.str;

public class StringMethod2 {
	
	public static void main(String[] args) {
		
		String s = "InmakesInfotech";
		
		char charAt = s.charAt(14);
		System.out.println(charAt);
		
		String s1 = "SeleniumAutomationtool";
		
		char charAt1 = s.charAt(7);
		System.out.println(charAt1);
		
		String s2 = "PenealSamuel";
		
		char charAt2 = s.charAt(9);
		System.out.println(charAt2);
		
		String s3 = "j a v a p r o g r a m";
		
		char charAt3 = s.charAt(8);
		System.out.println(charAt2); 
		
	}

}
